        <source_script><![CDATA[answer = (function transformEntry(source) {

	var loc = source. u_site_name +'';
if(loc.includes('CORP')){
     return "00000";
}
else{
var loc1 = loc.match(/\d+/)+'';

if(loc1.length == 5)
return loc1;

else if(loc1.length == 4){
return "0"+loc1;
}
else if(loc1.length == 3){
return "00"+loc1;
}
else if(loc1.length == 2){
return "000"+loc1;
}
else if(loc1.length == 1){
return "0000"+loc1;
}
}

})(source);]]></source_script>
